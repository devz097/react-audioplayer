{
  "name": "react-h5-audio-player",
  "version": "2.2.1",
  "description": "A React audio player with UI. Written in TypeScript. Mobile compatible.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "run-p build:js build:d.ts build:css build:less",
    "build:js": "run-s build:es build:cjs",
    "build:cjs": "cross-env NODE_ENV=cjs babel src --out-dir lib --extensions .tsx",
    "build:es": "cross-env NODE_ENV=es babel src --out-dir es --extensions .tsx",
    "build:d.ts": "tsc --rootDir src --outDir lib -d",
    "build:css": "sass --no-source-map src/styles.scss lib/styles.css",
    "build:less": "sass2less src/styles.scss lib/styles.less",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 9000",
    "build:storybook": "build-storybook -c .storybook -o react-h5-audio-player-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lhz516/react-h5-audio-player.git"
  },
  "keywords": [
    "react",
    "component",
    "music",
    "audio",
    "player",
    "mobile",
    "typescript"
  ],
  "author": "Hanz Luo",
  "license": "MIT",
  "eslintIgnore": [
    "lib",
    "react-h5-audio-player-storybook"
  ],
  "bugs": {
    "url": "https://github.com/lhz516/react-h5-audio-player/issues"
  },
  "homepage": "https://github.com/lhz516/react-h5-audio-player#readme",
  "peerDependencies": {
    "react": "^15.5.0 || ^16.0.0",
    "react-dom": "^15.5.0 || ^16.0.0",
    "prop-types": "^15.5.0 || ^16.0.0"
  },
  "dependencies": {
    "@iconify/icons-mdi": "^1.0.46",
    "@iconify/react": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.0",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-react": "^7.8.0",
    "@babel/preset-typescript": "^7.8.0",
    "@storybook/addon-actions": "^5.3.1",
    "@storybook/addon-docs": "^5.3.1",
    "@storybook/addon-options": "^5.3.1",
    "@storybook/react": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "less-plugin-sass2less": "^1.2.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.1",
    "typescript": "^3.7.4"
  }
}
